{
  "expo": {
    "name": "Entregador",
    "slug": "entregador",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",

    "userInterfaceStyle": "light",
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "icon": "./src/assets/icon.png",
      "bundleIdentifier": "com.anonymous.entregador",

      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": ["ACCESS_NETWORK_STATE", "ACCESS_WIFE_STATE", "INTERNET"],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.entregador"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ca0f3a90-7d85-46b1-8d9f-13ffa3c2ae04"
      }
    },
    "owner": "thiago_morgado"
  }
}
